{
  "name": "teamcowboy.js",
  "version": "0.0.0-semantically-released",
  "description": "A TypeScript first TeamCowboy API client",
  "module": "dist/esm/index.js",
  "main": "dist/cjs/index.js",
  "types": "dist/esm/index.d.ts",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "run-p 'build:*'",
    "build:cjs": "tsc -p ./tsconfig.cjs.json",
    "build:esm": "tsc -p ./tsconfig.json",
    "build:types": "tsc -d",
    "clean": "rimraf dist",
    "lint": "run-s 'lint:*'",
    "lint:es": "eslint src --max-warnings=0",
    "lint:style": "prettier 'src/**/*.(ts|js)' --check",
    "test": "jest"
  },
  "repository": "https://github.com/smoak/teamcowboy.js.git",
  "keywords": [
    "teamcowboy",
    "api",
    "request"
  ],
  "author": "Sasha Moak (https://github.com/smoak)",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.7.7",
    "axios-curlirize": "^1.3.7",
    "crypto-js": "^4.2.0",
    "qs": "^6.13.0",
    "universal-user-agent": "^6.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.4",
    "@babel/preset-typescript": "^7.24.7",
    "@semantic-release/exec": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^9.2.1",
    "@semantic-release/npm": "^11.0.0",
    "@types/axios-curlirize": "^1.3.2",
    "@types/crypto-js": "^4.2.2",
    "@types/fetch-mock": "^7.3.8",
    "@types/jest": "^29.5.13",
    "@types/node": "^18.17.1",
    "@types/qs": "^6.9.8",
    "@typescript-eslint/eslint-plugin": "^6.6.0",
    "@typescript-eslint/parser": "^6.6.0",
    "babel-jest": "^29.7.0",
    "eslint": "^8.48.0",
    "fetch-mock": "^9.3.1",
    "jest": "^29.7.0",
    "msw": "^1.3.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.3.3",
    "rimraf": "^5.0.1",
    "semantic-release": "^22.0.5",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">= 18"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "tag": "latest",
    "provenance": true
  },
  "volta": {
    "node": "18.17.1"
  }
}
